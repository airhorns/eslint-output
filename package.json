{
  "name": "eslint-output",
  "version": "0.0.4",
  "description": "export multiple formats of eslint",
  "keywords": [
    "eslint",
    "multiple",
    "outputs",
    "format",
    "formats",
    "wrapper"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lwhiteley/eslint-output"
  },
  "author": "Layton Whiteley",
  "license": "MIT",
  "private": false,
  "bin": {
    "eslint-output": "./app.js"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:outputs": "node app.js"
  },
  "dependencies": {
    "debug": "^4.1.0",
    "rc": "^1.2.8",
    "write": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.14.0"
  },
  "peerDependencies": {
    "eslint": ">=3"
  }
}
